<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.GreyMatters.GreyMattersApp">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.GreyMatters.GreyMattersApp.Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.GreyMatters.GreyMattersApp.EventList">
        </activity>
        <activity android:name="com.GreyMatters.GreyMattersApp.MainActivity">
        </activity>
        <activity android:name="com.GreyMatters.GreyMattersApp.EventOverview">
        </activity>
        <activity android:name="com.GreyMatters.GreyMattersApp.NewsList">
        </activity>
        <activity android:name="com.GreyMatters.GreyMattersApp.ContactUs">
        </activity>
        <activity android:name="com.GreyMatters.GreyMattersApp.PersonalCalendar">
        </activity>
    </application>

</manifest>